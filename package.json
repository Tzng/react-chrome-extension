{
  "name": "react-chrome-extension",
  "version": "0.0.1",
  "description": "Boilerplate for Chrome extension React.js project",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint src chrome __tests__ scripts config/*.js",
    "gulp": "gulp watch-all",
    "remove": "rimraf node_modules",
    "test-e2e": "cross-env NODE_ENV=test mocha -r __tests__/setup-app __tests__/e2e",
    "test": "cross-env NODE_ENV=test mocha -r __tests__/setup-app __tests__/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tzng/react-chrome-extension.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Jhen <developer@jhen.me>",
  "license": "MIT",
